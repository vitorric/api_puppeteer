{
  "name": "validacao-api",
  "version": "1.0.0",
  "main": "index.js",
  "author": "Vitor Ricardo",
  "license": "MIT",
  "scripts": {
    "build": "rm -rf dist && NODE_ENV=$npm_config_stage babel src --extensions \".js,.ts\" --out-dir dist --copy-files --no-copy-ignored",
    "postbuild": "cp package.json dist/package.json",
    "build:prod": "npm run build --stage=production",
    "build:hom": "npm run build --stage=homolog",
    "dev": "NODE_ENV=dev tsnd -r tsconfig-paths/register --transpile-only --respawn --ignore-watch node_modules --no-notify src/server.ts",
    "dockerbuild:dev": "docker-compose --env-file .env.dev build",
    "dockerup:dev": "docker-compose --env-file .env.dev up",
    "ts-check": "tsc",
    "prepare": "husky install"
  },
  "resolutions": {
    "luxon": "^3.2.1"
  },
  "dependencies": {
    "axios": "^1.4.0",
    "cors": "^2.8.5",
    "dotenv": "^16.0.1",
    "express": "^4.18.2",
    "helmet": "^7.0.0",
    "morgan": "^1.10.0",
    "passport": "^0.6.0",
    "passport-http": "^0.3.0",
    "passport-http-bearer": "^1.0.1",
    "puppeteer": "^24.3.0",
    "uuid": "^11.1.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.22.5",
    "@babel/core": "^7.22.5",
    "@babel/preset-env": "^7.22.5",
    "@babel/preset-typescript": "^7.22.5",
    "@types/cors": "^2.8.13",
    "@types/dotenv": "^8.2.0",
    "@types/helmet": "^4.0.0",
    "@types/morgan": "^1.9.4",
    "@types/node": "^20.3.1",
    "@types/passport": "^1.0.12",
    "@types/passport-http": "^0.3.11",
    "@types/passport-http-bearer": "^1.0.37",
    "@types/passport-jwt": "^4.0.1",
    "@typescript-eslint/eslint-plugin": "^5.33.0",
    "@typescript-eslint/parser": "^5.33.0",
    "babel-plugin-module-resolver": "^5.0.0",
    "eslint": "^8.21.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-google": "^0.14.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-import-resolver-typescript": "^3.4.0",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-import-helpers": "^1.3.1",
    "eslint-plugin-prettier": "^4.2.1",
    "prettier": "^2.7.1",
    "ts-node-dev": "^2.0.0",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.1.3"
  }
}
